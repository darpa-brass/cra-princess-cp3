OCAMLLIBDEPS+=$(PARSETREESDIR)/libparsetrees.$(OCAMLLIBEXT)

OCAMLINCS+=-I $(PARSERSDIR)
OCAMLLIBS+=$(PARSERSDIR)/libparsers.$(OCAMLLIBEXT)
OCAMLLIBDEPS+=$(PARSERSDIR)/libparsers.$(OCAMLLIBEXT)

include $(GENTOP)/mk/ocaml.mk
